{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "BASE_URL": "@base_url",
    "CHANNEL_USERNAME": "@channel_username"
  }
}
